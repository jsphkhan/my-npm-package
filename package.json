{
  "name": "@jsphkhan/my-npm-package",
  "version": "2.1.0",
  "description": "My first NPM package",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest ./_tests",
    "coverage": "jest --coverage ./_tests",
    "build": "./node_modules/.bin/babel src -d dist",
    "lint": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsphkhan/my-npm-package.git"
  },
  "author": "Joseph",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsphkhan/my-npm-package/issues"
  },
  "homepage": "https://github.com/jsphkhan/my-npm-package#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "husky": "^4.2.3",
    "jest": "^25.2.0",
    "lint-staged": "^10.0.9"
  },
  "lint-staged": {
    "*.js": [
      "npm test",
      "npm run coverage",
      "npm run lint",
      "npm run build"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "publishConfig": { "registry": "https://npm.pkg.github.com/" }
}
